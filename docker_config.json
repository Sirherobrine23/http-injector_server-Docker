{
    "name": "docker_runTeste",
    "docker_image": "teste/docker:run",
    "dockerfile": "Dockerfile",
    "ports": [
        {
            "port": 22,
            "external": 2222,
            "protocoll": "tcp"
        },
        {
            "port": 2122,
            "protocoll": "udp"
        }
    ],
    "mounts": [
        {
            "path": "/home/http",
            "from": "./docker/http"
        }
    ],
    "env": [
        {
            "name": "teste",
            "value": "google is best"
        }
    ],
    "options": [
        "--privileged"
    ]
}